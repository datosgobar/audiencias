<%= javascript_tag do %>
  $(function() {
    audiencias.app.init();
    audiencias.globals.currentObligee = <%= raw @current_obligee.id %>;
    audiencias.globals.currentUser = <%= raw @current_user.id %>;
    audiencias.globals.currentAudience = <%= raw @current_audience.id %>;
    audiencias.globals.obligees.add(<%= raw @current_obligee.to_json %>);
    audiencias.globals.users.add(<%= raw @current_user.to_json %>)
    var currentAudience = <%= raw @current_audience.to_json %>;
    currentAudience.editingApplicant = false;
    currentAudience.editingInfo = false;
    audiencias.globals.audiences.updateAudience(currentAudience);
    audiencias.app.audienceEditor();
  });
<% end %>