<%= javascript_tag do %>
  $(function() {
    <% if @search_results %>
      audiencias.globals.results = <%= raw @search_results.to_json %>;
    <% end %>
    <% if @aggregations %>
      audiencias.globals.aggregations = <%= raw @aggregations.to_json %>;
    <% end %>
    <% if @audience %>
      audiencias.globals.singleAudience = <%= raw @audience.to_json(for_public: true) %>
    <% end %>
    audiencias.app.searcher();
    <% if @current_user %>
      audiencias.globals.currentUser = <%= raw @current_user.id %>;
      audiencias.globals.users.add(<%= raw @current_user.to_json %>);
    <% end %>
  });
<% end %>